    "preferred_width": 400
  },
  "shortcuts": [
    {
      "name": "Ruido Blanco",
      "short_name": "Blanco",
      "description": "Iniciar ruido blanco",
      "url": "/?sound=WHITE_NOISE",
      "icons": [{"src": "/icons/icon-192.png", "sizes": "192x192"}]
    },
    {
      "name": "Ruido Rosa",
      "short_name": "Rosa",
      "description": "Iniciar ruido rosa",
      "url": "/?sound=PINK_NOISE",
      "icons": [{"src": "/icons/icon-192.png", "sizes": "192x192"}]
    },
    {
      "name": "Océano",
      "short_name": "Océano",
      "description": "Sonido del océano",
      "url": "/?sound=OCEAN",
      "icons": [{"src": "/icons/icon-192.png", "sizes": "192x192"}]
    }
  ],
  "protocol_handlers": [
    {
      "protocol": "web+nanapp",
      "url": "/?action=%s"
    }
  ],
  "share_target": {
    "action": "/",
    "method": "GET",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
